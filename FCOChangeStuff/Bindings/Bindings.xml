<Bindings>
    <Layer name="SI_KEYBINDINGS_CATEGORY_GENERAL">
        <Category name="|c00FF00FCO |cFFFF00Change Stuff|r">
            <Action name="FCOCS_ADDON_SETTINGS_MENU">
                <Down>FCOCS.openLAMAddonSettings(self)</Down>
            </Action>
            <Action name="FCOCS_MAP_PLAYER_PIN_PING_PONG">
                <Down>FCOCS.playerPinPingPong(true)</Down>
            </Action>
            <Action name="FCOCS_TOGGLE_SETTINGS_COMPASS_QUEST_GIVERS">
                <Down>FCOCS.keybinds("FCOCS_TOGGLE_SETTINGS_COMPASS_QUEST_GIVERS")</Down>
            </Action>
            <Action name="FCOCS_TOGGLE_AUDIO_MUTE_SFX">
                <Down>FCOCS.muteSFXSound()</Down>
            </Action>
            <Action name="FCOCS_TOGGLE_GROUP_ELECTION_AUTO_DECLINE">
                <Down>FCOCS.toggleGroupElectionAutoDecline()</Down>
            </Action>
        </Category>
    </Layer>
</Bindings>